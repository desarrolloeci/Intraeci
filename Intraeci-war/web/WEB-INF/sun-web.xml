<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 Servlet 2.4//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_4-1.dtd">
<sun-web-app error-url="">
  <context-root>/Intraeci-WebModule</context-root>
  <ejb-ref>
    <ejb-ref-name>ejb/InicioProfBean</ejb-ref-name>
    <jndi-name>ejb/InicioProfBean</jndi-name>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/LoginBean</ejb-ref-name>
    <jndi-name>ejb/LoginBean</jndi-name>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/BDprofesorBean</ejb-ref-name>
    <jndi-name>ejb/BDprofesorBean</jndi-name>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/BDadministrativoBean</ejb-ref-name>
    <jndi-name>ejb/BDadministrativoBean</jndi-name>
  </ejb-ref>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="classdebuginfo" value="true">
      <description>Enable debug info compilation in the generated servlet class</description>
    </property>
    <property name="mappedfile" value="true">
      <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
    </property>
  </jsp-config>
</sun-web-app>
